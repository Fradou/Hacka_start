<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    <div class="form-group">
        {{ form_row(form.first_name, {label:'First name :', 'attr':{'class' : 'form-control'}}) }}
    </div>
    <div class="form-group">
        {{ form_row(form.last_name, {label:'Last name :', 'attr':{'class' : 'form-control'}}) }}
    </div>
    <div class="form-group">
        {{ form_row(form.username, {label:'Username :', 'attr':{'class' : 'form-control'}}) }}
    </div>
    <div class="form-group">
        {{ form_row(form.email, {label:'Email :', 'attr':{'class' : 'form-control'} })}}
    </div>
    <div class="form-group">
        {{ form_row(form.plainPassword.first, {label:'Password :', 'attr':{'class' : 'form-control'}}) }}
    </div>
    <div class="form-group">
        {{ form_row(form.plainPassword.second, {label:'Confirm password :', 'attr':{'class' : 'form-control'}}) }}
    </div>
    <div class="form-group">
        {{ form_row(form.paypal_account, {label:'Paypal account :', 'attr':{'class' : 'form-control'}}) }}
    </div>
    <div class="form-group">
        {{ form_row(form.native_language, {label:'Native language :', 'attr':{'class' : 'form-control'}}) }}
    </div>
    <div class="form-group">
        {{ form_row(form.nationality, {label:'Nationality :', 'attr':{'class' : 'form-control'}}) }}
    </div>

    {{ form_widget(form) }}
    <div>
        <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" />
    </div>
</form>
